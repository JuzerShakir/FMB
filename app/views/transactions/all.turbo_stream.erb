<% if @transactions.any? %>
  <%= turbo_stream.append :transactions, partial: "transactions" %>
<% else %>
  <%= turbo_stream.append :transactions, partial: "shared/no_results" %>
<% end %>

<%= render "shared/pagy", path: transactions_all_path(format: :turbo_stream, page: @pagy.next, q: params[:q]&.to_unsafe_h) %>