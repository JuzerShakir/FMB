<% if @sabeels.any? %>
  <%= turbo_stream.append :sabeels, partial: "sabeels" %>
<% else %>
  <%= turbo_stream.append :sabeels, partial: "shared/no_results" %>
<% end %>

<%= render partial: "shared/pagy", formats: :html,
  locals: { path: sabeels_path(format: :turbo_stream, page: @pagy.next, q: params[:q]&.to_unsafe_h)}
%>