<%# * Title %>
<h2 class="text-center">Thaalis in <span class="text-secondary"><%= @year.to_s %></span> </h2>

<%# * Total No. of Thaalis %>
<p class="fs-5 mt-5 fw-bold text-info text-center">Total number of Thaalis: <%= @total %></p>

<%# * show all thaalis %>
<%= render partial: "shared/results", locals: {
  instances: @thaalis,
  id: "thaalis",
  path: thaalis_all_path(format: :turbo_stream)
} %>
